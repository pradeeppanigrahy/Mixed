                            @Html.Label("lblTestTransactionVal", Model.TestTransaction ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                        </div>
                        </div>
                    </div>
                </div>
            <div class="col-md-4 transinfo" >
                <div class="row">

                    <div class="form-group">
                        <div class="row">
                            <label for="user_title" class="control-label col-xs-4" name="lblCreatedBy">Trans created by:</label>
                            @Html.Label("lblCreatedByVal", Model.CreatedBy ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <label for="user_title" class="control-label col-xs-4" name="lblLiabilityType">Liability type:</label>
                            @Html.Label("lblLiabilityTypeVal", Model.LiabilityTypeDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <label for="user_title" class="control-label col-xs-4" name="lblDescription">Description:</label>
                            @Html.Label("lblDescriptionVal", Model.TransactionDescription ?? string.Empty, new { @class = "col-xs-5 trans_details description_txt" })
                        </div>
                        </div>
                    </div>
                </div>
     
        
   
}
