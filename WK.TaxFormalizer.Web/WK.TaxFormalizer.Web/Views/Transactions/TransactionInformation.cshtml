                     @Html.Label("lblCanRejectDeliveryVal", Model.SitusInformationModel.CanRejectDeliveryDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                 </div>
             </div>
             <div class="form-group">
                 <div class="row">
                     <label for="user_title" class="control-label col-xs-4"  name="lblShipFromPOB">Ship from POB:</label>
                     @Html.Label("lblShipFromPOBVal", Model.SitusInformationModel.ShipFromPOBDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                 </div>
                 </div>
             </div>
         </div>
            <div class="col-md-4 transinfo">
                <div class="row">

                    <div class="form-group">
                        <div class="row">
                            <label for="user_title" class="control-label col-xs-4" name="lblStatus">Status:</label>
                            @Html.Label("lblStatusVal", Model.TransactionStatusDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <label for="user_title" class="control-label col-xs-4" name="lblShipFromGeocode">Ship from geocode:</label>
                            @Html.Label("lblShipFromGeocodeVal", Model.SitusInformationModel.ShipFromGeocode ?? string.Empty, new { @class = "col-xs-5 trans_details" })
