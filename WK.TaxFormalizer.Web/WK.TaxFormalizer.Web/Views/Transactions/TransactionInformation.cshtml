@model WK.TaxFormalizer.Core.Models.TransactionInformationModel
@{
    Layout = null;
}
@if (Model != null)
{

   <style>

       .container {
           margin-left:-10px !important;
       }

     

      
   </style>

        
        <div class="col-md-4">
            <div class="row">

                <div class="form-group">
                    <div class="row">
                        <label for="user_title" class="control-label col-xs-4" name="lblDate">Reporting date:</label>
                        @Html.Label("lblDateVal", Model.TransactionDate.HasValue ? Model.TransactionDate.Value.ToString("MM/dd/yyyy") : string.Empty, new { @class = "col-xs-5 trans_details" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <label for="user_title" class="control-label col-xs-4" name="lblId">Transaction #:</label>
                        @Html.Label("lblIdVal", Convert.ToString(Model.TransactionId), new { @class = "col-xs-5 trans_details", @id = "TransactionId" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <label for="user_title" class="control-label col-xs-4" name="lblAmount">Transaction amt:</label>
                        @Html.FormatAmountLabel("lblAmountVal", Model.Amount, 3, "col-xs-5 trans_details")
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <label for="user_title" class="control-label col-xs-4" name="lblTax">Total tax amt:</label>
                        @Html.FormatAmountLabel("lblTaxVal", Model.TotalTaxAmount, 3, "col-xs-5 trans_details")
                    </div>
                    </div>
                </div>
            </div>
    <div class="col-md-4 transinfo ">
        <div class="row">
            <div class="form-group">
                <div class="row">
                    <label for="user_title" class="control-label col-xs-4" name="lblType">Transaction type:</label>
                    @Html.Label("lblTypeVal", Model.TransactionTypeDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                </div>
            </div>

            <div class="form-group">
                <div class="row">

                    <label for="user_title" class="control-label col-xs-4" name="lblInvoiceDate">Invoice date:</label>
                    @Html.Label("lblInvoiceDateVal", Model.InvoiceDate.HasValue ? Model.InvoiceDate.Value.ToString("MM/dd/yyyy") : string.Empty, new { @class = "col-xs-5 trans_details" })
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <label for="user_title" class="control-label col-xs-4" name="lblInvoiceId">Invoice ID:</label>
                    @Html.Label("lblInvoiceIdVal", Model.InvoiceId ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <label for="user_title" class="control-label col-xs-4" name="lblOriginalId">Original trans #:</label>
                    @Html.Label("lblOriginalIdVal", Convert.ToString(Model.OriginalTransactionId), new { @class = "col-xs-5 trans_details" })
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4 transinfo ">
        <div class="row">
            <div class="form-group">
                <div class="row">
                    <label for="user_title" class="control-label col-xs-4" name="lblProviderType">Provider type:</label>
                    @Html.Label("lblProviderTypeVal", Model.ProviderTypeDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <label for="user_title" class="control-label col-xs-4" name="lblUsageType">Customer/Usage type:</label>
                    @Html.Label("lblUsageTypeVal", Model.CustomerOrUsageTypeDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <label for="user_title" class="control-label col-xs-4" name="lblSourceSystem">Source system:</label>
                    @Html.Label("lblSourceSystemVal", Model.SourceSystem ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <label for="user_title" class="control-label col-xs-4" name="lblCustomerId">Customer/Vendor ID:</label>
                    @Html.Label("lblCustomerIdVal", Model.VendorID ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                </div>
            </div>
        </div>
    </div>
     <div class="col-md-4">
         <div class="row">

             <div class="form-group">
                 <div class="row">
                     <label for="user_title" class="control-label col-xs-4" name="lblFOB">FOB:</label>
                     @Html.Label("lblFOBVal", Model.SitusInformationModel.FOBDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
                 </div>
             </div>
             <div class="form-group">
                 <div class="row">
                     <label for="user_title" class="control-label col-xs-4" name="lblMailOrder">Mail order:</label>
                     @Html.Label("lblMailOrderVal", Model.SitusInformationModel.MailOrderDescription ?? string.Empty, new { @class = "col-xs-5 trans_details" })
